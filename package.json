    "markdownlint-cli": "^0.16.0",
    "prettier": "^1.17.1",
    "socket.io": "2.2.0",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "^5.16.0",
    "tslint-eslint-rules": "5.4.0",
    "typedoc": "0.14.2",
