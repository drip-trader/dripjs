    "@nestjs/core": "^6.2.4",
    "@nestjs/platform-express": "^6.2.4",
    "@nestjs/platform-socket.io": "^6.2.4",
    "@nestjs/websockets": "^6.2.4",
    "axios": "^0.21.1",
    "bignumber.js": "^8.1.1",
    "config": "^3.0.1",
    "dotenv": "^8.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mysql": "^2.16.0",
    "qs": "^6.6.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "^6.4.0",
